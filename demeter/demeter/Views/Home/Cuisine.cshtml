<body>

    <div class="loading-part">
        <div class="loader-home">Loading...</div>
    </div>

    <div id="wrapper">

        <div class="lines-overlay"></div>
        <div id="main">
            <!-- Header -->
            <header id="header">
                <a href="/Home/Welcome"><img src="https://img.icons8.com/ios/50/000000/double-left-filled.png" style="width: 100px; height: 100px"></a>
                <a href="/Home/Business"><img src="https://img.icons8.com/android/100/000000/back.png" style="width: 100px; height: 100px"></a>
                <br />
                <br />
                <br />
                <h1>Select a Cuisine</h1>
                <br />
                @*<p id="text-alert" class="animated-quick"></p>*@
                <nav>
                    <ul>
                        <li>
                            <a href="#" class="messages icon-popup icon"><img src="~/Content/assets/img/turkish.png" style="width: 100px; height: 100px;" /></a>
                            <br />
                            <br />
                            @*<span class="notification opacity-0 animated-quick">1</span>*@
                            <h4 class="name">Turkish</h4>
                        </li>

                        <li>
                            <a href="#" class="about icon-popup icon"><img src="~/Content/assets/img/lebanese.png" style="width: 100px; height: 100px;" /></a>
                            <br />
                            <br />
                            <h4 class="name">Lebanese</h4>
                        </li>

                        <li>
                            <a href="#" class="photos icon-popup icon"><img src="~/Content/assets/img/middleEastern.png" style="width: 100px; height: 100px;" /></a>
                            <br />
                            <br />
                            <h4 class="name">Middle East</h4>
                        </li>
                    </ul>
                </nav>
            </header>

        </div>

    </div>

