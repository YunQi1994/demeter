    <!-- * Libraries jQuery and Bootstrap - Be careful to not remove them * -->
    <script src="~/Content/assets/js/jquery.min.js"></script>

    <script src="~/Content/assets/js/bootstrap.min.js"></script>

    <!-- Countdown -->
    <script src="~/Content/assets/js/jquery.lwtCountdown-1.0.js"></script>

    <!-- Newsletter plugin -->
    <script src="~/Content/assets/js/notifyMe.js"></script>

    <!-- Contact form verification -->
    <script src="~/Content/assets/js/contact-me.js"></script>

    <!-- Swipebox plugin -->
    <script src="~/Content/assets/js/jquery.swipebox.js"></script>

    <!-- Background plugin -->
    <script src="~/Content/assets/js/vegas.js"></script>

    <!-- Background settings -->
    <script>

        $('body').vegas({
            slides: [
                { src: '/Content/assets/img/turkishBackground.jpg' },
                { src: '/Content/assets/img/lebaneseBackground.jpg' },
                { src: '/Content/assets/img/middleEasternBackground.jpg' }
            ],

            // Delay beetween slides in milliseconds.
            delay: 5000,

            // Chose your transition effect (See the documentation provided in your download pack)
            transition: 'fade'
        });

    </script>

